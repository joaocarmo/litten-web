  "homepage": "https://litten.app",
  "private": true,
  "main": "src/pages/index.js",
  "repository": "git@github.com:joaocarmo/litten-web.git",
  "author": "Joao Carmo <17721880+joaocarmo@users.noreply.github.com>",
  "contributors": [],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint:fix": "eslint --fix . && stylelint ./src/**/*.scss --fix",
    "lint": "eslint . && stylelint ./src/**/*.scss",
    "postinstall": "patch-package",
    "pretty:fix": "prettier --write .",
    "pretty": "prettier --check .",
    "push": "git add . && gitmoji -c && git push",
    "start": "gatsby develop --port 8001"
  },
  "browserslist": {
